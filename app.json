{
  "expo": {
    "name": "Sumee Pro",
    "slug": "sumee-pro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#820AD1"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sumeeapp.pro",
      "infoPlist": {
        "NSCameraUsageDescription": "Sumee Pro necesita acceso a la cámara para capturar documentos de identidad y fotos de perfil.",
        "NSLocationWhenInUseUsageDescription": "Tu ubicación se utiliza para mostrarte trabajos cercanos y guiarte al sitio del servicio.",
        "NSPhotoLibraryUsageDescription": "Permite acceder a la galería para subir documentos de certificación."
      }
    },
    "android": {
      "package": "com.sumeeapp.pro",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#820AD1"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.FOREGROUND_SERVICE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "Permite que Sumee Pro acceda a tus fotos para completar tu expediente."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite que Sumee Pro use tu ubicación para encontrarte trabajos relevantes incluso en segundo plano."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#6D28D9",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4f45a619-abba-4e47-a9af-b4f2e16e25bf"
      }
    }
  }
}